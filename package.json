{
    "name": "easynce",
    "version": "1.2.2",
    "description": "一个简单的低代码平台项目",
    "main": "index.js",
    "packageManager": "pnpm@8.7.1",
    "type": "module",
    "engines": {
        "pnpm": ">=8.7.1",
        "node": ">=20.13.1"
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "cz": "pnpm git cz",
        "in": "pnpm i -r",
        "ui": "pnpm -F @easynce/ui dev",
        "clean": "rimraf --glob ./node_modules ./pnpm-lock.yaml ./packages/**/node_modules ./packages/**/**/node_modules ./packages/**/dist",
        "build:ui": "pnpm -F @easynce/ui build",
        "play:vue3": "pnpm -F @easynce/playground-vue3 dev",
        "prepare": "husky",
        "log": "pnpm conventional-changelog -p ./changelog.config.js -i CHANGELOG.md -s -r 0"
    },
    "keywords": [
        "vue3",
        "element-plus",
        "vite",
        "no-code",
        "easynce"
    ],
    "author": "bbnocode",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/bibinocode/easynce"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.4.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/eslint-config-typescript": "^13.0.0",
        "commitizen": "^4.3.0",
        "conventional-changelog": "^6.0.0",
        "conventional-changelog-cli": "^5.0.0",
        "cz-vinyl": "^2.5.1",
        "eslint": "^8.57.0",
        "eslint-plugin-vue": "^9.23.0",
        "husky": "^9.1.4",
        "lint-staged": "^15.2.8",
        "prettier": "^3.2.5",
        "rimraf": "^6.0.1"
    },
    "lint-staged": {
        "*.{js,ts,vue}": "eslint"
    },
    "config": {
        "commitizen": {
            "path": "cz-vinyl"
        }
    }
}
